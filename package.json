{
  "name": "react-router-redux-boilerplate",
  "version": "1.0.0",
  "engines": {
    "node": "6.x.x",
    "npm": "3.x.x"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run build:data && npm run scripts:client && npm run scripts:server && npm run scripts:views && npm run styles",
    "build:data": "babel src/data -sd dist/data --ignore *_spec.js",
    "scripts:client": "webpack --progress --colors",
    "scripts:server": "babel src/server -sd dist/server --ignore *_spec.js",
    "scripts:views": "babel src/views -sd dist/views --ignore *_spec.js",
    "styles": "node-sass --output-style compressed src/styles/default.sass dist/client/default.css",
    "watch": "npm run watch:client & npm run watch:data & npm run watch:server & npm run watch:views & npm run watch:styles",
    "watch:client": "webpack --progress --colors --watch --config webpack.local.js",
    "watch:data": "babel src/data -wsd dist/data",
    "watch:server": "babel src/server -wsd dist/server",
    "watch:views": "babel src/views -wsd dist/views",
    "watch:styles": "nodemon -e sass -x \"npm run styles\"",
    "start": "nodemon dist/server"
  },
  "dependencies": {
    "hapi": "16.1.0",
    "inert": "^4.1.0",
    "install": "^0.8.4",
    "nodemon": "1.11.0",
    "npm": "^4.1.2",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "react-redux": "4.4.6",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "serialize-javascript": "1.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "5.0.4",
    "babel-loader": "6.2.10",
    "babel-plugin-module-alias": "1.6.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-1": "6.22.0",
    "babel-register": "6.22.0",
    "eslint": "1.10.3",
    "eslint-config-rackt": "1.1.1",
    "eslint-plugin-react": "3.16.1",
    "express": "4.14.0",
    "node-sass": "4.4.0",
    "redux-devtools": "3.3.2",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.2.0",
    "styled-components": "1.4.1",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0"
  }
}
